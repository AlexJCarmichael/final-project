<h1>Dashboard for <%= current_user.user_name %></h1>

<div class='dash-display'>
  <h4 id='clicky-button'>Characters</h4>
  <div class='hide-on-small-only' id='expand-me'>
    <p><%= @character.first.name %></p>
    <p><%= @character.first.stats.first.name %> | <%= @character.first.stats.first.value %></p>
    <p><%= @character.first.stats.second.name %> | <%= @character.first.stats.second.value %></p>
</div>
