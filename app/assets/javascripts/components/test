handlePost: function () {
  var that = this;
  var foo = "foobar"
  var dataToSend = {};
  dataToSend["game_session"] = {}

  $.ajax({
    method: "POST",
    url: "/" + this.props.controller + ".json",
    data: dataToSend,
    success: function(response) {
      window.location.replace("/" + that.props.redirect_controller + "/" + response.redirect_id);
    }
  }).done(function(response) {
    that.setState({
      postText: ''
    });
  });
},

data: {
  game_session: {
    session_name: this.state.postText
  }
},
